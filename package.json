{
  "name": "@soral/string-pixel-width",
  "version": "1.12.1",
  "description": "String pixel width measurement on the backend in Javascript",
  "keywords": [
    "string",
    "width",
    "pixel width"
  ],
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "author": "soraliu <soraliu.dev@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/soraliu/string-pixel-width.git"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org",
    "tag": "latest"
  },
  "files": [
    "/dist"
  ],
  "browserslist": [
    "cover 99.9%"
  ],
  "scripts": {
    "prepare": "rm -rf dist && npm run build",
    "build": "run-p build:esm build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs",
    "test": "jest"
  },
  "dependencies": {
    "lodash.deburr": "^4.1.0"
  },
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@types/lodash.deburr": "^4.1.6",
    "@types/node": "^17.0.1",
    "jest": "^27.5.1",
    "npm-run-all": "^4.1.5",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.4.0",
    "tslib": "^2.3.1",
    "typescript": "^4.4.4"
  }
}
